<div id="<%= dom_id meeting %>">
  <p>
    <strong>Name:</strong>
    <%= meeting.name %>
    <span style="margin-left: 10px; margin-right: 10px;">|</span>
    <strong>Date:</strong>
    <%= meeting.date %>
    <span style="margin-left: 10px; margin-right: 10px;">|</span>
    <strong>Time:</strong>
    <%= meeting.time %>
    <span style="margin-left: 10px; margin-right: 10px;">|</span>
    <strong>Location:</strong>
    <%= meeting.location %>
  </p>
  <p>
    <strong>Description:</strong>
    <%= meeting.description %>
  </p>
</div>
